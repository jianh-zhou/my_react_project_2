{
  "compilerOptions": {
    "paths": {
      "@pages/*": [
        "./src/pages/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@comp/*": [
        "./src/components/*"
      ]
    },
    "baseUrl": ".",
    "module": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}